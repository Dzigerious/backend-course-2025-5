    "url": "https://github.com/Dzigerious/backend-course-2025-5/issues"
  },
  "homepage": "https://github.com/Dzigerious/backend-course-2025-5#readme",
  "dependencies": {
    "commander": "^14.0.2",
    "superagent": "^10.2.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
